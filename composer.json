{
    "name": "alhoqbani/smsa-webservice",
    "type": "library",
    "description": "smsaexpress.com tracking web service (soap api)",
    "keywords": [
        "alhoqbani",
        "smsa-webservice"
    ],
    "homepage": "https://github.com/alhoqbani/smsa-webservice",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamoud Alhoqbani",
            "email": "h.alhoqbani@gmail.com",
            "homepage": "https://github.com/alhoqbani",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Alhoqbani\\SMSAWebService\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alhoqbani\\SMSAWebService\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
